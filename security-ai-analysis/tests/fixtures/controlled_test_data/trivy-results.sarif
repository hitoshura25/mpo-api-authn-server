{
  "version": "2.1.0",
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "fullName": "Trivy Vulnerability Scanner",
          "informationUri": "https://github.com/aquasecurity/trivy",
          "name": "Trivy",
          "rules": [
            {
              "id": "CVE-2024-0001",
              "name": "OsPackageVulnerability",
              "shortDescription": {
                "text": "lodash: Prototype pollution vulnerability allows remote code execution"
              },
              "fullDescription": {
                "text": "Prototype pollution vulnerability in lodash allows remote code execution through malicious payload"
              },
              "defaultConfiguration": {
                "level": "error"
              },
              "helpUri": "https://nvd.nist.gov/vuln/detail/CVE-2024-0001",
              "properties": {
                "precision": "very-high",
                "security-severity": "8.5",
                "tags": [
                  "vulnerability",
                  "security",
                  "HIGH"
                ]
              }
            },
            {
              "id": "CVE-2024-0002",
              "name": "OsPackageVulnerability",
              "shortDescription": {
                "text": "express: Middleware vulnerability allows request smuggling"
              },
              "fullDescription": {
                "text": "Express.js middleware vulnerability allows request smuggling attacks"
              },
              "defaultConfiguration": {
                "level": "warning"
              },
              "helpUri": "https://nvd.nist.gov/vuln/detail/CVE-2024-0002",
              "properties": {
                "precision": "very-high",
                "security-severity": "6.1",
                "tags": [
                  "vulnerability",
                  "security",
                  "MEDIUM"
                ]
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "CVE-2024-0001",
          "level": "error",
          "message": {
            "text": "Package: lodash, Installed Version: 4.17.19, Fixed Version: 4.17.21"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "package-lock.json"
                },
                "region": {
                  "startLine": 1,
                  "startColumn": 1
                }
              }
            }
          ]
        },
        {
          "ruleId": "CVE-2024-0002",
          "level": "warning",
          "message": {
            "text": "Package: express, Installed Version: 4.16.0, Fixed Version: 4.18.2"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "package-lock.json"
                },
                "region": {
                  "startLine": 1,
                  "startColumn": 1
                }
              }
            }
          ]
        }
      ]
    }
  ]
}